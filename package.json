{
  "name": "sym-ts",
  "version": "0.1.0",
  "description": "A TypeScript implementation of symbolic mathematics with multiple backend options",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "example:basic": "tsx examples/basic.ts",
    "example:webassembly": "tsx examples/webassembly.ts",
    "example:advanced": "tsx examples/advanced-features.ts",
    "example:optimized": "tsx examples/optimized-demo.ts"
  },
  "keywords": [
    "symbolic",
    "mathematics",
    "computer-algebra",
    "typescript",
    "webassembly",
    "webgpu",
    "sympy"
  ],
  "author": "Max Schurig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxschurig/sym-ts.git"
  },
  "bugs": {
    "url": "https://github.com/maxschurig/sym-ts/issues"
  },
  "homepage": "https://github.com/maxschurig/sym-ts#readme",
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "big.js": "^6.2.1"
  },
  "optionalDependencies": {
    "@webgpu/types": "^0.1.38"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
